const Prj = [
  {
    title: "Analista de Siporte en sitio ",
    description:
      "Revision de tickets, soporte a usuarios, configuracion de equipos y redes.",
    alt: "Noteworthy technology acquisitions 2021",
    date: "2024 / 08 - Now",
  },
  {
    title: "Callcenter - soporte tecnico",
    description:
      "Atencion ded PQR en soporte e intentar solventar los inconvenientes que presernten los usuarios.",
